{
  "rulesDirectory": [
    "node_modules/tslint-consistent-codestyle/rules",
    "node_modules/tslint-clean-code/dist/src",
    "node_modules/rxjs-tslint-rules/dist/rules",
    "node_modules/tslint-sonarts/lib/rules",
    "node_modules/tslint-eslint-rules/dist/rules",
    "node_modules/eslint-plugin-node/lib/rules",
    "node_modules/eslint-plugin-es/lib/rules",
    "node_modules/eslint-plugin-security/rules"
  ],
  "jsRules": {
    "no-empty": true
  },
  "rules": {
    "use-default-type-parameter": true,
    "prefer-object-spread": true,
    "prefer-conditional-expression": true,
    "no-this-assignment": true,
    "no-sparse-arrays": true,
    "no-return-await": true,
    "no-inferred-empty-object-type": true,
    "no-invalid-template-strings": true,
    "no-misused-new": true,
    "no-invalid-this": true,
    "no-var-requires": true,
    "no-parameter-reassignment": true,
    "no-non-null-assertion": true,
    "no-magic-numbers": [true, -1, 0, 1, 2, 3, 4, 5],
    "no-import-side-effect": true,
    "no-reference": true,
    "no-unnecessary-type-assertion": true,
    "ordered-imports": true,
    "no-constant-condition": true,
    "no-control-regex": true,
    "no-duplicate-case": true,
    "no-empty-character-class": true,
    "no-ex-assign": true,
    "no-extra-boolean-cast": true,
    "no-extra-semi": true,
    "no-inner-declarations": true,
    "no-invalid-regexp": true,
    "ter-no-irregular-whitespace": true,
    "no-regex-spaces": true,
    "ter-no-sparse-arrays": true,
    "no-unexpected-multiline": true,
    "no-multi-spaces": true,
    "ter-no-proto": true,
    "ter-no-script-url": true,
    "ter-no-self-compare": true,
    "valid-typeof": true,
    "valid-jsdoc": [false],
    "early-exit": [true, { "max-length": 3 }],
    "no-accessor-recursion": true,
    "no-collapsible-if": true,
    "no-return-undefined": true,
    "no-else-after-return": true,
    "no-static-this": true,
    "no-unused": [true, { "ignore-pattern": "^_" }],
    "no-var-before-return": true,
    "callable-types": true,
    "class-name": true,
    "no-for-each-push": true,
    "no-feature-envy": false,
    "no-map-without-usage": true,
    "no-complex-conditionals": true,
    "prefer-dry-conditionals": true,
    "no-flag-args": true,
    "min-class-cohesion": false,
    "rxjs-ban-observables": true,
    "rxjs-ban-operators": true,
    "rxjs-no-do": { "severity": "warning" },
    "rxjs-no-tap": { "severity": "warning" },
    "rxjs-no-ignored-subscribe": true,
    "rxjs-no-unsafe-catch": true,
    "rxjs-no-unsafe-first": true,
    "rxjs-no-unsafe-scope": true,
    "rxjs-no-unsafe-switchmap": true,
    "rxjs-no-unsafe-takeuntil": true,
    "rxjs-no-nested-subscribe": true,
    "rxjs-no-wholesale": false,
    "rxjs-no-internal": true,
    "rxjs-no-subject-unsubscribe": true,
    "rxjs-no-subject-value": true,
    "rxjs-no-unbound-methods": true,
    "deprecation": { "severity": "warning" },
    "rxjs-finnish": {
      "options": [
        {
          "functions": false,
          "methods": false,
          "parameters": true,
          "properties": true,
          "variables": true
        }
      ],
      "severity": "error"
    },
    "no-all-duplicated-branches": true,
    "no-case-with-or": true,
    "no-collection-size-mischeck": true,
    "no-element-overwrite": true,
    "no-empty-destructuring": true,
    "no-identical-conditions": true,
    "no-identical-expressions": true,
    "no-ignored-initial-value": true,
    "no-ignored-return": true,
    "no-in-misuse": true,
    "no-misleading-array-reverse": true,
    "no-misspelled-operator": true,
    "no-self-assignment": true,
    "no-unthrown-error": true,
    "no-use-of-empty-return-value": true,
    "no-useless-increment": true,
    "no-useless-intersection": true,
    "cognitive-complexity": [true, 15],
    "consecutive-overloads": true,
    "max-union-size": [true, 3],
    "mccabe-complexity": [true, 20],
    "no-accessor-field-mismatch": true,
    "no-array-delete": true,
    "no-big-function": [true, 200],
    "no-commented-code": true,
    "no-dead-store": true,
    "no-duplicate-in-composite": true,
    "no-duplicated-branches": true,
    "no-duplicate-string": [true, 3],
    "no-empty-nested-blocks": true,
    "no-extra-semicolon": true,
    "no-gratuitous-expressions": true,
    "no-hardcoded-credentials": true,
    "no-identical-functions": true,
    "no-inconsistent-return": true,
    "no-invalid-await": true,
    "no-invariant-return": true,
    "no-inverted-boolean-check": true,
    "no-multiline-string-literals": true,
    "no-nested-incdec": true,
    "no-nested-template-literals": true,
    "no-redundant-boolean": true,
    "no-redundant-jump": true,
    "no-redundant-parentheses": true,
    "no-return-type-any": true,
    "no-same-line-conditional": true,
    "no-small-switch": true,
    "no-statements-same-line": true,
    "no-undefined-argument": true,
    "no-unenclosed-multiline-block": true,
    "no-unused-array": true,
    "no-useless-cast": true,
    "no-variable-usage-before-declaration": true,
    "parameters-max-number": [true, 5],
    "prefer-default-last": true,
    "prefer-immediate-return": true,
    "prefer-optional": true,
    "prefer-promise-shorthand": true,
    "use-primitive-type": true,
    "use-type-alias": true,
    "comment-format": [true, "check-space"],
    "curly": true,
    "eofline": true,
    "forin": true,
    "import-blacklist": [true],
    "import-spacing": true,
    "indent": [true, "spaces"],
    "interface-over-type-literal": true,
    "label-position": true,
    "max-line-length": [true, 140],
    "member-access": false,
    "member-ordering": [
      true,
      {
        "order": [
          "public-static-field",
          "private-static-field",
          "public-instance-field",
          "private-instance-field",
          "public-constructor",
          "private-constructor",
          "public-instance-method",
          "protected-instance-method",
          "private-instance-method"
        ]
      }
    ],
    "no-arg": true,
    "no-bitwise": true,
    "no-console": [true, "debug", "info", "time", "timeEnd", "trace"],
    "no-construct": true,
    "no-debugger": true,
    "no-empty": false,
    "no-empty-interface": true,
    "no-eval": true,
    "no-inferrable-types": [true],
    "no-shadowed-variable": false,
    "no-string-literal": false,
    "no-string-throw": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": true,
    "no-unused-expression": true,
    "no-use-before-declare": true,
    "no-var-keyword": true,
    "object-literal-sort-keys": false,
    "one-line": [true, "check-open-brace", "check-catch", "check-else", "check-whitespace"],
    "prefer-const": true,
    "quotemark": [true, "single"],
    "radix": true,
    "semicolon": [true, "always"],
    "triple-equals": [true, "allow-null-check"],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "unified-signatures": true,
    "variable-name": false,
    "whitespace": [true, "check-branch", "check-decl", "check-operator", "check-separator", "check-type"],
    "directive-selector": [true, "attribute", "app", "camelCase"],
    "component-selector": [true, "element", "app", "kebab-case"],
    "use-input-property-decorator": true,
    "use-output-property-decorator": true,
    "use-host-property-decorator": true,
    "no-input-rename": true,
    "no-output-rename": true,
    "use-life-cycle-interface": true,
    "use-pipe-transform-interface": true,
    "component-class-suffix": true,
    "directive-class-suffix": true
  }
}
